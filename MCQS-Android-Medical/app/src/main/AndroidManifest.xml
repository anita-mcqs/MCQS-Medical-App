<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mcqs.anita.mcqs_android_version1" >

    <uses-permission android:name="android.permission.INTERNET" />




    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >


        <activity
            android:name=".ViewQuestion"
            android:label="@string/title_activity_view_question" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChooseExam"
            android:label="@string/title_activity_choose_exam" >
        </activity>
        <activity
            android:name=".DownloadExam"
            android:label="@string/title_activity_single_view" >
        </activity>
    </application>

</manifest>
